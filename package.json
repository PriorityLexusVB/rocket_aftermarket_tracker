  "scripts": {
    "dev": "vite",
    "dev:5174": "vite --port 5174 --strictPort",
    "generate:icons": "node scripts/generate-icons.mjs",
    "guard:client-env": "node scripts/guard-client-env.mjs",
    "guard:instructions": "node scripts/guard-instructions.mjs",
    "test": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run",
    "test:watch": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs",
    "test:ui": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs --ui",
    "verify": "pnpm -s guard:client-env && pnpm -s guard:instructions && pnpm -s lint && pnpm -s typecheck && pnpm -s test",
    "start": "vite",
    "start:runner": "node scripts/runVite.js",
    "build": "vite build --sourcemap",
    "preview": "vite preview",
    "serve": "vite preview",
    "postinstall": "pnpm list supabase > /dev/null 2>&1 && pnpm rebuild supabase || echo 'Skipping supabase rebuild (not installed)'",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "typecheck": "tsc -p tsconfig.e2e.json --noEmit",
    "dev:create-test-user": "node scripts/bootstrapTestUser.js",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "PWDEBUG=1 playwright test",
    "release:check": "node scripts/releaseChecklist.mjs",
    "db:push": "npx supabase@latest db push",
    "db:audit": "npx supabase@latest db query ./scripts/sql/audit_security_surface.sql",
    "db:seed-org": "npx supabase@latest db query ./scripts/attach_org.sql",
    "db:seed-e2e": "node scripts/seedE2E.js",
    "db:cleanup-e2e": "node scripts/cleanupE2E.js",
    "db:report-e2e": "node scripts/reportE2E.js",
    "db:print-org-id": "node scripts/printOrgId.js",
    "e2e:seed": "pnpm -s db:seed-e2e",
    "e2e:schema:apply": "DOTENV_CONFIG_PATH=.env.e2e.local node -r dotenv/config scripts/applyLineItemSchedulingMigration.mjs",
    "cleanup:old-deals": "node scripts/cleanupOldDeals.cjs",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:studio": "drizzle-kit studio",
    "sb:whoami": "bash scripts/supabase/whoami.sh",
    "sb:link:test": "bash scripts/supabase/link-env.sh test",
    "sb:link:prod": "bash scripts/supabase/link-env.sh prod",
    "sb:migration:list:test": "bash scripts/supabase/with-env.sh test -- npx -y supabase@latest migration list --linked",
    "sb:migration:list:prod": "bash scripts/supabase/with-env.sh prod -- npx -y supabase@latest migration list --linked",
    "sb:db:push:test": "bash scripts/supabase/with-env.sh test -- npx -y supabase@latest db push",
    "sb:db:push:test:all": "bash scripts/supabase/with-env.sh test -- npx -y supabase@latest db push --include-all",
    "sb:db:push:prod": "bash scripts/supabase/with-env.sh prod -- npx -y supabase@latest db push",
    "sb:db:push:prod:all": "bash scripts/supabase/with-env.sh prod -- npx -y supabase@latest db push --include-all"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
