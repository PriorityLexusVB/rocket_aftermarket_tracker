  "scripts": {
    "dev": "vite",
    "guard:client-env": "node scripts/guard-client-env.mjs",
    "test": "vitest run",
    "start": "vite",
    "start:runner": "node scripts/runVite.js",
    "build": "vite build --sourcemap",
    "preview": "vite preview",
    "serve": "vite preview",
    "postinstall": "pnpm list supabase > /dev/null 2>&1 && pnpm rebuild supabase || echo 'Skipping supabase rebuild (not installed)'",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "typecheck": "tsc -p tsconfig.e2e.json --noEmit",
    "dev:create-test-user": "node scripts/bootstrapTestUser.js",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "PWDEBUG=1 playwright test",
    "release:check": "node scripts/releaseChecklist.mjs",
    "db:push": "npx supabase@latest db push",
    "db:audit": "npx supabase@latest db query ./scripts/sql/audit_security_surface.sql",
    "db:seed-org": "npx supabase@latest db query ./scripts/attach_org.sql",
    "db:seed-e2e": "node scripts/seedE2E.js",
    "db:cleanup-e2e": "node scripts/cleanupE2E.js",
    "db:report-e2e": "node scripts/reportE2E.js",
    "db:print-org-id": "node scripts/printOrgId.js",
    "e2e:seed": "pnpm -s db:seed-e2e",
    "e2e:schema:apply": "DOTENV_CONFIG_PATH=.env.e2e.local node -r dotenv/config scripts/applyLineItemSchedulingMigration.mjs",
    "cleanup:old-deals": "node scripts/cleanupOldDeals.cjs",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:studio": "drizzle-kit studio",
    "guard:client-env": "node scripts/guardClientEnv.mjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "4.7.0",
    "autoprefixer": "10.4.22",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.2.0",
    "pg": "^8.13.1",
    "playwright": "^1.57.0",
    "postcss": "8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "supabase": "^2.65.6",
    "tailwindcss": "3.4.18",
    "typescript": "^5.9.3",
    "vite": "5.4.21",
    "vite-tsconfig-paths": "3.6.0",
