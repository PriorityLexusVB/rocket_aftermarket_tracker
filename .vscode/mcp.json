{
	"servers": {
		"github/github-mcp-server": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp",
			"version": "0.13.0"
		},
		"supabase": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@supabase/mcp-server-supabase@0.5.10",
				"--project-ref",
				"ntpoblmjxfivomcwmjrj",
				"--features",
				"account,docs,database,debugging,development,functions,storage,branching",
				"--api-url",
				"https://api.supabase.com"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "${input:SUPABASE_ACCESS_TOKEN}",
				"npm_config_yes": "true"
			},
			"version": "0.5.10"
		},
		"microsoft/playwright-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"env": {
				"npm_config_yes": "true"
			},
			"version": "0.0.1-seed"
		}
	},
	"inputs": [
		{
			"id": "SUPABASE_ACCESS_TOKEN",
			"type": "promptString",
			"description": "Personal access token for Supabase API",
			"password": true
		}
	]
}