{
  "servers": {
    "github/github-mcp-server": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp"
    },
    "supabase-e2e": {
      "type": "stdio",
      "command": "bash",
      "args": ["${workspaceFolder}/scripts/mcp/supabase-mcp.sh", ".env.e2e.local"],
      "env": {
        "npm_config_yes": "true"
      }
    },
    "supabase-dev": {
      "type": "stdio",
      "command": "bash",
      "args": ["${workspaceFolder}/scripts/mcp/supabase-mcp.sh", ".env.local"],
      "env": {
        "npm_config_yes": "true"
      }
    },
    "supabase": {
      "type": "stdio",
      "command": "bash",
      "args": ["${workspaceFolder}/scripts/mcp/supabase-mcp.sh", ".env.e2e.local"],
      "env": {
        "npm_config_yes": "true"
      }
    },
    "microsoft/playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {
        "npm_config_yes": "true"
      }
    }
  },
  "inputs": []
}
